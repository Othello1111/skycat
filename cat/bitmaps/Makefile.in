INSTALL      = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@
CAT_DIR      = @CAT_DIR@
BINDIR       = @BINDIR@
RM           = rm -f
SHELL        = /bin/sh
FILES        = *.xbm *.xpm 
srcdir	     = @srcdir@
TARGET       = ../src/cat_bitmaps.C

all: $(TARGET)

All: 
	cd ..; $(MAKE)

# generate a C file that creates all of the bitmap files
# in this directory and installs them for Tk access by basename
$(TARGET): $(FILES)
	tclsh@TCL_VERSION@  bitmaps.tcl > $(TARGET)

install:

clean:
	$(RM) *\~ "#"* core

depend:

distclean: clean
	rm -f Makefile	

man:

include @SRC_ROOT@/makefile.mk

